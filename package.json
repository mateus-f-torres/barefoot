{
  "name": "barefoot",
  "version": "2.0.0",
  "description": "React Redux SPA boilerplate",
  "author": "mateus-f-torres",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "8.11.4",
    "npm": "5.6.0"
  },
  "scripts": {
    "build:check": "yarn lint && yarn flow:status && yarn test",
    "build:remote": "yarn build:check && webpack --config webpack.development.config.js",
    "build:remote-force": "webpack --config webpack.development.config.js",
    "build:local": "yarn build:check && webpack-dev-server --hot --open --config webpack.development.config.js",
    "build:local-force": "webpack-dev-server --hot --open --config webpack.development.config.js",
    "build:deploy": "yarn build:check && webpack --config webpack.production.config.js",
    "build:deploy-force": "webpack --config webpack.production.config.js",
    "serve:parse": "babel src/server.js -o ./server.js",
    "serve:remote": "yarn serve:parse && NODE_ENV=development node server.js",
    "serve:deploy": "yarn serve:parse && NODE_ENV=production node server.js",
    "start": "NODE_ENV=production node server.js",
    "lint": "eslint ./",
    "flow:status": "flow status",
    "flow:stop": "flow stop",
    "flow:warn": "flow status --include-warnings",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "polished": "^2.0.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "styled-components": "^3.4.6",
    "styled-normalize": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-styled-components": "^1.7.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^2.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.5.0",
    "eslint-config-google": "^0.10.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.80.0",
    "flow-coverage-report": "^0.6.0",
    "flow-typed": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.0",
    "react-test-renderer": "^16.4.2",
    "redux-devtools": "^3.4.1",
    "redux-mock-store": "^1.5.3",
    "simple-progress-webpack-plugin": "^1.1.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.18.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  }
}
