{
  "name": "barefoot",
  "version": "4.0.0",
  "description": "React Redux SPA boilerplate",
  "author": "mateus-f-torres",
  "homepage": "https://mateus-f-torres.github.io/barefoot/",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "start:mock": "json-server __mocks__/api.json",
    "start:storybook": "start-storybook",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "serve": "http-server dist -g",
    "format": "prettier --write \"{src,cypress,__{tests,mocks}__}/**/*.js\"",
    "lint": "eslint \"{src,cypress,__{tests,mocks}__}/**/*.js\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:cypress": "cypress open"
  },
  "dependencies": {
    "i18next": "^19.1.0",
    "normalize.css": "^8.0.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-i18next": "^11.3.2",
    "react-redux": "7.1.3",
    "redux": "4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-transform-async-to-generator": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rckeller/cypress-unfetch": "^1.0.1",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-viewport": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@storybook/theming": "^5.3.13",
    "@testing-library/cypress": "^5.3.0",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.0",
    "autoprefixer": "9.7.4",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "commitizen": "^4.0.3",
    "compression-webpack-plugin": "3.1.0",
    "core-js": "3.6.4",
    "css-loader": "3.4.2",
    "cypress": "^4.0.2",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-cypress": "^2.10.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-standard": "4.0.1",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.12.1",
    "husky": "^4.2.3",
    "image-webpack-loader": "6.0.0",
    "jest": "25.1.0",
    "jest-fetch-mock": "^3.0.1",
    "json-server": "0.16.0",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-custom-properties": "9.0.2",
    "postcss-loader": "3.0.0",
    "prettier": "1.19.1",
    "simple-progress-webpack-plugin": "1.1.2",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "url-loader": "3.0.0",
    "webpack": "4.41.6",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
