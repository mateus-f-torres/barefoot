{
  "name": "barefoot",
  "version": "2.2.0",
  "description": "React Redux SPA boilerplate",
  "author": "mateus-f-torres",
  "homepage": "https://mateus-f-torres.github.io/barefoot/",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "local": "webpack-dev-server --hot --open --config webpack.development.config.js",
    "prebuild": "eslint src/ && flow status && jest",
    "build": "webpack --config webpack.production.config.js",
    "deploy": "gh-pages -d dist",
    "prestart": "NODE_ENV=development babel src/server.js -o ./server.js",
    "start": "NODE_ENV=production node server.js",
    "lint:src": "eslint src/",
    "lint:all": "eslint ./",
    "flow:status": "flow status",
    "flow:stop": "flow stop",
    "flow:warn": "flow status --include-warnings",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "compression": "1.7.3",
    "express": "4.16.4",
    "polished": "2.3.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "redux": "4.0.1",
    "styled-components": "4.1.3",
    "styled-normalize": "8.0.4"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-styled-components": "1.10.0",
    "clean-webpack-plugin": "1.0.0",
    "compression-webpack-plugin": "2.0.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.11.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.0",
    "file-loader": "3.0.1",
    "flow-bin": "0.89.0",
    "flow-coverage-report": "0.6.1",
    "flow-typed": "2.5.1",
    "gh-pages": "2.0.1",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.6.0",
    "jest": "23.6.0",
    "jest-styled-components": "6.3.1",
    "react-test-renderer": "16.7.0",
    "redux-devtools": "3.5.0",
    "redux-mock-store": "1.5.3",
    "simple-progress-webpack-plugin": "1.1.2",
    "svg-url-loader": "2.3.2",
    "uglifyjs-webpack-plugin": "2.1.1",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14"
  }
}
