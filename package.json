{
  "name": "barefoot",
  "version": "4.5.0",
  "description": "React Redux PWA Kitchen Sink",
  "author": "mateus-f-torres",
  "homepage": "https://mateus-f-torres.github.io/barefoot/",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
