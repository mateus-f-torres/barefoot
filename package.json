  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "start:mock": "json-server __mocks__/api.json",
    "start:storybook": "start-storybook",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "serve": "http-server ./dist -b -P http://localhost:8080?",
    "format": "prettier --write \"{src,cypress,__{tests,mocks}__}/**/*.js\"",
    "lint": "eslint \"{src,cypress,__{tests,mocks}__}/**/*.js\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:cypress": "cypress open",
    "test:cypress-run": "cypress run"
  },
