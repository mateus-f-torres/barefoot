{
  "name": "barefoot",
  "version": "2.2.0",
  "description": "React Redux SPA boilerplate",
  "author": "mateus-f-torres",
  "homepage": "https://mateus-f-torres.github.io/barefoot/",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "publishConfig": {
    "registry": ""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "prebuild": "yarn lint && yarn typecheck",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "deploy": "gh-pages -d dist",
    "preremote": "NODE_ENV=production babel src/server.js -o ./server.js",
    "remote": "NODE_ENV=production node server.js",
    "local": "serve dist/",
    "lint": "eslint src/ --ext js,jsx",
    "typecheck": "flow check --include-warnings src/",
    "test": "jest",
    "postinstall": "flow-typed install"
  },
  "dependencies": {
    "compression": "1.7.3",
    "express": "4.16.4",
    "polished": "2.3.3",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "redux": "4.0.1",
    "styled-components": "4.1.3",
    "styled-normalize": "8.0.4"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-async-to-generator": "7.2.0",
    "@babel/plugin-transform-flow-strip-types": "7.2.3",
