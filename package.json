{
  "name": "barefoot",
  "version": "0.2.0",
  "description": "boilerplate for M.E.R.N SPAs",
  "author": "mateus-f-torres",
  "license": "MIT",
  "private": true,
  "scripts": {
    "predev": "NODE_ENV=development && yarn lint && yarn test && yarn run babel dev_server.js -o server.js",
    "dev": "webpack --progress --config webpack.development.config.js",
    "prestart": "NODE_ENV=development && yarn lint && yarn test",
    "start": "webpack-dev-server --config  webpack.development.config.js --hot --open",
    "prebuild": "NODE_ENV=production && yarn lint && yarn test && yarn run babel prod_server -o server.js",
    "build": "webpack --progress --config webpack.production.config.js",
    "serve": "node server.js",
    "eject": "",
    "lint": "eslint ./ --ext .js --ext .jsx",
    "status": "yarn run flow status",
    "stop": "yarn run flow stop",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.16.3",
    "package.json": "^2.0.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-hot-loader": "^1.4.0",
    "css-loader": "^0.28.11",
    "empty": "^0.10.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.75.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-loader": "^2.1.5",
    "redux-devtools": "^3.4.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
