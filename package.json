{
  "name": "barefoot",
  "version": "3.0.0",
  "description": "React Redux SPA boilerplate",
  "author": "mateus-f-torres",
  "homepage": "https://mateus-f-torres.github.io/barefoot/",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "mock": "json-server __mocks__/api.json",
    "start": "webpack-dev-server --config webpack.config.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "predeploy": "yarn test",
    "deploy": "gh-pages -d dist",
    "preremote": "NODE_ENV=production babel src/server.js -o ./server.js",
    "remote": "NODE_ENV=production node server.js",
    "local": "http-server dist -g",
    "format": "prettier --write \"{src,__{tests,mocks}__}/**/*.js\"",
    "lint": "eslint \"{src,__{tests,mocks}__}/**/*.js\" --fix",
    "test": "jest --coverage"
  },
  "dependencies": {
    "compression": "1.7.4",
    "express": "4.17.1",
    "i18next": "^17.0.11",
    "normalize.css": "^8.0.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-i18next": "^10.12.2",
    "react-redux": "7.1.0",
    "redux": "4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/node": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-transform-async-to-generator": "7.5.0",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@testing-library/jest-dom": "4.0.0",
    "@testing-library/react": "9.1.3",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "commitizen": "^4.0.3",
    "compression-webpack-plugin": "3.0.0",
    "core-js": "3.2.1",
    "css-loader": "3.2.0",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-standard": "13.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-standard": "4.0.0",
    "file-loader": "4.2.0",
    "gh-pages": "2.1.1",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "husky": "^3.0.4",
    "image-webpack-loader": "5.0.0",
    "jest": "24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "json-server": "0.15.0",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "0.8.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-custom-properties": "9.0.2",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "simple-progress-webpack-plugin": "1.1.2",
    "style-loader": "1.0.0",
    "svg-url-loader": "3.0.0",
    "terser-webpack-plugin": "1.4.1",
    "url-loader": "2.1.0",
    "webpack": "4.39.2",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
