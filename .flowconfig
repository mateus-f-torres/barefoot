[ignore]
# r => react*
# s => styled-components
<PROJECT_ROOT>/node_modules/[^rs]

[include]

[libs]

[lints]
# all=off by default
sketchy-null=warn
untyped-import=warn
unclear-type=warn

[options]
# ignore next line flow error
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
# add module wrappers for webpack alias
# e.g.
# module.name_mapper='^Images' -> '<PROJECT_ROOT>/src/assets/images'

[strict]
