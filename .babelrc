{
  "env": {
    "development": {
      "presets": [
        "env",
        "react",
        "flow",
      ],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
      ],
    },
    "production": {
      "presets": [
        "env",
        "react",
        "flow",
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties",
        "transform-react-constant-elements",
        "remove-test-ids",
        {
          "attributes": ["data-test"]
        }
      ],
    },
    "test": {
      "presets": [
        "env",
        "react",
        "flow",
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties",
      ],
    },
  },
}
